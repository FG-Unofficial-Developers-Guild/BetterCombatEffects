<?xml version="1.0" encoding="iso-8859-1"?>

<root>
	<windowclass name="ct_effect" merge="join">
		<sheetdata>
			<button_ct_effect_apply name="apply" merge="join">
				<anchored to="leftanchor" width="24" height="10">
					<top offset="0"/>
					<left anchor="right" relation="relative" offset="2" />
				</anchored>
					<parameters>
						<icons>button_toggle_action|button_toggle_roll|button_toggle_single|button_toggle_duse</icons>
						<values>action|roll|single|duse</values>
						<tooltipsres>effect_tooltip_applyone|effect_tooltip_applyroll|effect_tooltip_applysingle|effect_tooltip_disableuse</tooltipsres>
					</parameters>
			</button_ct_effect_apply>
			<button_ct_effect_changestate name="changestate" />
		</sheetdata>
	</windowclass>
</root>
